<script type="text/javascript">
	$(document).ready(function () {
		$("ul.Custom_list01").find("a").click(function() {
			var index = $(this).parents("div.Custom_content").index();
			fn_changeTab(index);
		});
	});
	
	// Tab 변경 Function
	function fn_changeTab(index) {
		$("div.Custom_sch").find("div.Custom_content").eq(index).hide();
		
		if($("div.Custom_sch").find("div.Custom_content").eq(index + 1).length > 0) {
			$("div.Custom_sch").find("div.Custom_content").eq(index + 1).show();
		}
	}
	
	// 'X' Button Click Event Function
	function btn_FirstLayerClose() {
		$("div.Custom_sch_wrap").fadeOut(300);
		
		$("body").css({
	           overflow: 'inherit'
	      	});
	}
	
	// '이전으로' Button Click Event Function
	function fn_backTab(index) {
		$("div.Custom_sch").find("div.Custom_content").eq(index-2).show();
		$("div.Custom_sch").find("div.Custom_content").eq(index-1).hide();
	}
	
	// 사용자 그룹 선택 View Function
	function fn_viewSubMenu(subMenu, userGrp, txt) {
		$("div.Custom_sch").find("#subMenu1").hide().html("");
		$("div.Custom_sch").find("#subMenu2").hide().html("");
		$("div.Custom_sch").find("#subMenu3").hide().html("");
		$("div.Custom_sch").find("#subMenu4").hide().html("");
		$("div.Custom_sch").find("#subMenu5").hide().html("");
		$("div.Custom_sch").find("#subMenu6").hide().html("");
		
		$("#spanChoice1").text(txt);
		
		var url = '/common/selectFxeList.do';
		var data = {
			'subMenu' : subMenu, 
			'txt' : txt,
			'userGrp' : userGrp
		};
		
		ajaxCall(url, 'POST', data, 'JSON', 'application/x-www-form-urlencoded', null, fn_viewSubMenuCallBack);
	}
	
	// 사용자 그룹 선택 Callback Function
	function fn_viewSubMenuCallBack(data, response) {
		
		if(data.result) {
			var htmlStr = "";
			var code = "";
			
			for(var i = 0; i <data.fxeList.length; i++) {
				
				var obj = data.fxeList[i];
				
				if(data.subMenu == "subMenu1") {			
					code = "S";
					
				} else if(data.subMenu == "subMenu2") {
					code = "J";
					
				} else if(data.subMenu == "subMenu3") {
					code = "R";
					
				} else if(data.subMenu == "subMenu4") {
					code = "O";
					
				} else if(data.subMenu == "subMenu5") {
					code = "W";
					
				} else if(data.subMenu == "subMenu6") {
					code = "E";
				}
				
				if(obj.pageUrl != null && obj.pageUrl.length > 0 && obj.pageUrl != "undefined") {
					htmlStr += "<li>";
					htmlStr += "<a href=\"javascript:fn_openChartLink('" + obj.pageUrl + "', '" + obj.pageName + "', '" + obj.menuId + "', '" + code + "');\" style=\"word-break: break-word; padding: 30px; \">";
					htmlStr += obj.pageName;
					htmlStr += "</a>";
					htmlStr += "</li>";
				}
			}
			
			$("div.Custom_sch").find("#" + data.subMenu).append(htmlStr);	
			
		} else {
			alert(data.msg);
		}
		
		$("div.Custom_sch").find("#" + data.subMenu).show(300);		
	}
	
	// Draw Chart Function 
	function fn_openChartLink(pageUrl, pageName, menuId, portalUserGrp)
	{
		$("#divChart").hide().html("");
		$("#linkButton").text(pageName + " 바로가기");
		$("#spanChoice2").text(pageName);
		$("#h2Choice2").text(pageName);
		
		if(name == "훈련기준검색") {	// 팝업 (팝업을 구분할 기준이 없음.)
			$("#linkButton").prop("href", "javascript:fn_linkPopUp('" + pageUrl + "','" + pageName + "');");
		
		} else {	// 일반화면
			$("#linkButton").prop("href", pageUrl);
		}	
		
		fn_changeTab(1);
		
		var options = {};
		options.contentType = "x-www-form-urlencoded";
		options.url = "/common/selectChartData.do" + "?menuId=" + menuId + "&portalUserGrp=" + portalUserGrp;
		options.success = fn_drawChart;
		
		gfn_ajax_request(options);
	}
	
	// Draw Chart Callback Function
	function fn_drawChart(data, response) {
		
		var names = new Array();
		var dataset = new Array();
		var rankArray = new Array();
		
		if(data != null && data.length > 0) {
			
			for(var i = data.length - 1; i >= 0; i--) {
				var vo = data[i];
				
				names.push(vo.baseYm);
				var dataArr = new Array();
				dataArr.push(vo.rank);
				dataset.push(dataArr);
				
				rankArray.push(vo.rank);
			}
		}  else {			
			for(var i = 0; i < 12;i++) {
				
				names.push(i + 1);
				
				var dataArr = new Array();
				dataArr.push(0);
				dataset.push(dataArr);
				
				rankArray.push(0);
			}
		}
		
		var maxValue = Math.max.apply(null, rankArray);
		var minValue = Math.min.apply(null, rankArray);
				
		var chartOptions = {
				'legend':{
					names: names /* ['08-12', '08-19', '08-26', '09-02', '09-09', '09-16', '9-23']  */
						},
				'dataset':{
					title:'월별 이용회수', 
					values: dataset,
					colorset: ['#DC143C'],
					fields:['회수']
				},
				'chartDiv' : 'divChart',
				'chartType' : 'line',
				'leftOffsetValue': 80,
				'bottomOffsetValue': 60,
				'chartSize' : {width:600, height:300},
				'minValue' : minValue,
				'maxValue' : maxValue,
				'increment' : Math.ceil(maxValue / 10),
				'isGuideLineNeeded' : true 
			};
	
		$("#divChart").html("");
		
		
		Nwagon.chart(chartOptions);
		$("#divChart").show(1000);
	}
	
	function fn_layerPopOpen(){
		$('body').css({
	           overflow: 'inherit'
	    });
		
		var PopView = $("div.Custom_sch_wrap");
		PopView.find("div.Black_bg").height($("body").height());
		var PopSet = PopView.find("div.Custom_sch");
		PopView.fadeIn(300);
		PopSet.css({
		    left : (($(window).width() - PopSet.outerWidth())/2+$(window).scrollLeft()) + "px",
		    top : (($(window).height() - PopSet.outerHeight())/2+$(window).scrollTop()) + "px"
		});
		PopSet.find("div.Custom_content").eq(0).show();
		var offset = PopView.offset();
		
		$(window).scroll(function(){
			var pos = (($(window).height() - PopSet.outerHeight())/2+$(window).scrollTop()) + "px";
			$("div.Custom_sch").css("top", pos);
		});
	
	}
		
	function fn_linkPopUp(pageUrl, pageName) {
		
		var popParam = {
			 	 "width"  : 1100
				,"height"  : 700
				,"Title"   : pageName
				};
		
	
		gfn_com_openPopup(pageUrl, popParam, null);
	}
	
	function numberWithComas(x){
		return x.toString().replace(/\B(?=(\d{3})+(?!1\d))/g,",");
	}
	
	
	
	
</script>
