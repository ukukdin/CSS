	// 직업리스트 조회 Callback Function
	function fn_JobInfoFindListCB(data, response) {
		$("#ul_job_list").empty();
		
		if(data != null && data.list.length > 0) {
			for(var i = 0; i < data.list.length; i++) {
				var innerHtml = "<li>";
				innerHtml += "<a jobCd=\"" + data.list[i].jobCd + "\" href=\"javascript:fn_JobInfoFindLink('" + data.list[i].jobCd + "');\">" + data.list[i].jobName + "</a>";
				innerHtml += "</li>";
				$("#ul_job_list").append(innerHtml);
			}
		
			// Draw Pagination
			$("#divJobFindPage").empty();
			var totPage = Math.ceil(Number(data.list[0].totCnt) / 24);
			var currPage = Number($("#jobFindPageIndex").val());
			var befPage = (currPage > 10) ? (currPage - 1) : 1;
			var nxtPage = (currPage + 10 < totPage) ? (currPage + 10) : totPage;
			var startIdx = Math.floor(currPage / 10) * 10 + 1;
			var endIdx = Math.floor(currPage / 10) * 10 + 10 >= totPage ? totPage : Math.floor(currPage / 10) * 10 + 10;
			
			var innerHtml = "<button onclick=\"javascript:fn_jobFindPageMove(1);\">";
			innerHtml += "<i class=\"xi-backward\"></i>";
			innerHtml += "<b class=\"blind\">처음 페이지</b>";
			innerHtml += "</button>";
			innerHtml += "<button onclick=\"javascript:fn_jobFindPageMove(" + befPage + ");\">";
			innerHtml += "<i class=\"xi-step-backward\"></i>";
			innerHtml += "<b class=\"blind\">이전 10페이지</b>";
			innerHtml += "</button>";
				
			for(var i = startIdx; i <= endIdx; i++) {
				if(currPage == i) {
					innerHtml += "<button onclick=\"javascript:fn_jobFindPageMove(" + i + ");\" class=\"On\">" + i + "</button>";
					
	    		} else {
	    			innerHtml += "<button onclick=\"javascript:fn_jobFindPageMove(" + i + ");\">" + i + "</button>";
	    		}
	    	}	
	  		   
			innerHtml += "<button onclick=\"javascript:fn_jobFindPageMove(" + nxtPage + ");\">";
			innerHtml += "<i class=\"xi-step-forward\"></i>";
			innerHtml += "<b class=\"blind\">다음 10페이지</b>";
			innerHtml += "</button>";
	    	innerHtml += "<button onclick=\"javascript:fn_jobFindPageMove(" + totPage + ");\">";
	    	innerHtml += "<i class=\"xi-forward\"></i>";
	    	innerHtml += "<b class=\"blind\">마지막 페이지</b>";
	    	innerHtml += "</button>";
	    	
	    	$("#divJobFindPage").append(innerHtml);
	    	$("#sch_result_cnt_job").text(data.list[0].totCnt);
		}
	}
	
