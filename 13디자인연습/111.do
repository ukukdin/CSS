	/* 분류별채용현황 데이터 정재 */
	function fn_parseChartData( chartDataArray ) {
	    var chartData = [];
	    var category = [];
	    var temp = [];
	    var dataArray;

	    for( var row in chartDataArray ) {
	        var data = chartDataArray[row];
	        var dataY = data["cnt"];
	        category.push( data["gbnm"] );

	        dataArray = {
	            name: data["gbnm"]
	            , data: [ { color : chartColorCode(data['gbnm']),  x: parseInt(row), y :  data["cnt"] } ]
	        }

	        temp.push(dataArray);
	    }

	    chartData.push( category );
	    chartData.push( temp );

	    return chartData;
	}
	
	/* 분류별채용현황 데이터 */
	function fn_drawStep00Chart(id, data) {
	    Highcharts.chart( id, {
	        chart: { type: 'column' }
	        , title: { text: '' }
	        , credits: { enabled: false }
	        , xAxis: {
	            startOnTick : true
	            , categories  : data[0]
	            , labels      : {
	                style: { fontSize: '13px', fontFamily: 'Verdana, sans-serif' },
	            }
	        }
	        , yAxis: {
	            min    : 0
	            , title  : { text: '' }
	            , labels : { formatter: function() { return Highcharts.numberFormat( this.value, 0 ); } }
	        }
	        , legend: {
	            enabled: false
	        }
	        , plotOptions: {
	            column: { stacking: 'normal' }
	            , series: {
	                dataLabels: {
	                    enabled  : true
	                    , rotation : -90
	                    , color    : '#FFFFFF'
	                    , align    : 'right'
	                    , format   : '{point.y:,.0f}'
	                    , y        : 10
	                    , style: { fontSize: '13px', fontFamily: 'Verdana, sans-serif' }
	                },
	            }
	        }
	        , series: data[1]
	    } );
	}
	
