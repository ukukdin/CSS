    scwin.isKeyDownShift = false;
    scwin.isKeyDownCtrl  = false;
    scwin.isKeyDownAlt   = false;
    
    $(document).keydown(function(e){
        e = e||window.event;
        var charCode = (e.which) ? e.which : event.keyCode;
        
        if(charCode == 16) scwin.isKeyDownShift = true;
        if(charCode == 17) scwin.isKeyDownCtrl  = true;
        if(charCode == 18) scwin.isKeyDownAlt   = true;
    });
    
    $(document).keyup(function(e){
        scwin.isKeyDownShift = false;
        scwin.isKeyDownCtrl  = false;
        scwin.isKeyDownAlt   = false;
    });
    
    $(document).ready(function(){
        if(window.opener){
            $(window).unload(function(){
                window.opener.gfn_com_handleHoldDiv(false);
            });
        }
    	
        if(typeof fn_initScreen == "function"){
            fn_initScreen();
        }
        
        
    });
    
</script>
	
